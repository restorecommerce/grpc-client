    "random-js": "^1.0.8"
  },
  "devDependencies": {
    "@types/long": "^4.0.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.11.6",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "nyc": "^14.1.1",
    "mocha": "^6.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
