    "sleep": "^6.0.0",
    "tslint": "^5.14.0",
    "typescript": "^3.3.4000"
  },
  "scripts": {
    "lint": "tslint './src/**/*.ts' './test/*.ts'",
    "pretest": "npm run tsctests",
